{
  "name": "backend",
  "version": "1.0.2",
  "description": "portal-backend",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node src/server.ts",
    "start": "ts-node ./dist/src/server.js",
    "prod": "pm2 start ./dist/src/server.js --watch",
    "runProd": "pm2-runtime ./dist/src/server.js",
    "nodemonjs": "nodemon --delay 3000ms ./dist/src/server.js",
    "build": "tsc -p .",
    "start1": "nodemon --exec npm run dev",
    "nodemonserver": "nodemon --delay 3000ms src/server.ts",
    "typeorm-commonjs": "typeorm-ts-node-commonjs",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f dist/src/database/config/ormconfig.js",
    "migration:show": "npm run typeorm:cli -- migration:show",
    "schema:log": "npm run build && npm run typeorm:cli -- schema:log",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate --config src/config/ormconfig.json --connection  --name ",
    "migration:run": "npm run typeorm -- migration:run",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/service-worker": "^15.0.2",
    "@google-cloud/spanner": "^6.1.3",
    "@okta/jwt-verifier": "^2.3.0",
    "@sap/hana-client": "^2.13.22",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-bearer-token": "^2.4.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "hdb-pool": "^0.1.6",
    "ip": "^1.1.8",
    "moment": "^2.29.4",
    "mongodb": "^3.7.3",
    "mssql": "^9.0.1",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.2",
    "node-mailer": "^0.1.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "npx": "^10.2.2",
    "pg": "^8.7.3",
    "pm2": "^5.2.0",
    "postgres-array": "^3.0.1",
    "python-shell": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "shelljs": "^0.8.5",
    "sql.js": "^1.7.0",
    "ssl-root-cas": "^1.3.1",
    "ts-node": "^10.9.1",
    "turbocommons-ts": "^3.8.0",
    "typeorm": "0.3.7",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.5",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
