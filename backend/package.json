{
  "name": "ih-portal-backend",
  "version": "1.0.170",
  "main": "index.js",
  "author": "TSOLEGNAGBO Kossi <kossi.tsolegnagbo@gmail.com>",
  "private": true,
  "description": "Integratehealth analytics backend app for chws followup",
  "homepage": "https://portal-integratehealth.org:9999",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KossiPascal/ih-portal.git"
  },
  "contributors": [
    "TSOLEGNAGBO Kossi <kossi.tsolegnagbo@gmail.com>"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/KossiPascal/ih-portal"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node src/server.ts",
    "prestart": "npm --no-git-tab-version version patch -m \"Upgrade to %s for reasons\"",
    "server1": "nodemon --delay 3000ms src/server.ts",
    "server2": "nodemon --delay 3000ms src/server2.ts",
    "start": "concurrently \"npm run server1\" \"npm run server2\"",
    "server": "ts-node ./dist/src/server.js",
    "prod": "pm2 start ./dist/src/server.js --watch",
    "runProd": "pm2-runtime ./dist/src/server.js",
    "runProd2": "pm2-runtime ./src/server.ts",
    "nodemonjs": "nodemon --delay 3000ms ./dist/src/server.js",
    "build": "tsc -p .",
    "start1": "nodemon --exec npm run dev",
    "nodemonserver": "nodemon --delay 3000ms src/server.ts",
    "typeorm-commonjs": "typeorm-ts-node-commonjs",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f dist/src/database/config/ormconfig.js",
    "migration:show": "npm run typeorm:cli -- migration:show",
    "schema:log": "npm run build && npm run typeorm:cli -- schema:log",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate --config src/config/ormconfig.json --connection  --name ",
    "migration:run": "npm run typeorm -- migration:run",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [],
  "license": "ISC",
