{
  "name": "ih-portal",
  "version": "1.2.45",
  "author": "TSOLEGNAGBO Kossi <kossi.tsolegnagbo@aiesec.net>",
  "private": true,
  "description": "Integratehealth analytics app for chws followup",
  "homepage": "https://portal-integratehealth.org:9999",
  "repository": "https://github.com/KossiPascal/ih-portal",
  "contributors": [
    {
      "name": "TSOLEGNAGBO Kossi",
      "email": "kossi.tsolegnagbo@aiesec.net"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/KossiPascal/ih-portal"
  },
  "scripts": {
    "ng": "ng serve",
    "start": "ng serve --ssl true",
    "start2": "ng serve --ssl true --ssl-key ./node_modules/browser-sync/lib/server/certs/server.key --ssl-cert ./node_modules/browser-sync/lib/server/certs/server.crt",
    "appreq": "ng build --service-worker=true --build-optimizer=true --optimization=true --configuration=production --aot=true --output-hashing=all",
    "start_dev": "concurrently \"npm run ng serve\" \"cd ../backend && npm run nodemonserver\"",
    "prebuild": "npm --no-git-tab-version version patch -m \"Upgrade to %s for reasons\"",
    "build": "ng build --service-worker --build-optimizer --optimization --configuration production --aot --output-hashing=all --named-chunks --stats-json",
    "postbuild": "cd ../ && git status && git add . && git status && git commit -m \"Initial Commit\" && git status && git push -u origin main && git status",
    "build2": "sudo rm -rf .angular/cache && ng build --service-worker --build-optimizer --optimization --configuration production --aot --deploy-url=https://portal-integratehealth.org:9999/ --extract-css=true ",
    "watch": "ng build --watch --configuration development",
    "precache": "sw-precache --verbose --config=sw-precache-config.js"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.1.2",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.1.2",
    "@angular/forms": "^14.0.0",
    "@angular/google-maps": "^14.2.7",
    "@angular/material": "^14.1.3",
    "@angular/material-moment-adapter": "^14.1.3",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^15.0.2",
    "@angular/pwa": "^15.1.0-next.0",
    "@angular/router": "^14.0.0",
    "@angular/service-worker": "^14.2.12",
    "@fortawesome/angular-fontawesome": "^0.10.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@highcharts/map-collection": "^2.0.1",
    "@ng-bootstrap/ng-bootstrap": "^14.0.0-rc.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "concurrently": "^7.3.0",
    "core-js": "^3.24.1",
    "dexie": "^3.2.2",
    "dexie-relationships": "^1.2.11",
    "highcharts": "^10.3.3",
    "highcharts-angular": "^3.1.2",
    "http-server": "^14.1.1",
    "lite-server": "^2.6.1",
    "moment": "^2.29.4",
    "morris": "^0.1.4",
    "ng-multiselect-dropdown": "^1.0.0-beta.19",
    "ng-pwa-tools": "0.0.15",
    "ngx-loadable": "^2.1.1",
    "ngx-multiple-dates": "^14.3.1",
    "postgres-array": "^3.0.1",
    "proj4": "^2.9.0",
    "rxjs": "~7.5.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "sw-precache": "^5.2.1",
    "tslib": "^2.3.0",
    "turbocommons-ts": "^3.8.0",
    "typescript": "^4.8.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular/cli": "^14.0.7",
    "@angular/compiler-cli": "^14.0.0",
    "@types/proj4": "^2.5.2",
    "browser-sync": "^2.29.0"
  }
